{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "hp4_paint:artist_villager",
      "materials": {
        "default": "villager_v2",
        "masked": "villager_v2_masked"
      },
      "textures": {
        "default": "textures/entity/villager2/villager",
        "zombie": "textures/entity/zombie/zombie"
      },
      "geometry": {
        "default": "geometry.villager_v2",
        "zombie": "geometry.zombie.v1.8"
      },
      "scripts": {
        "scale": "0.9375",
        "pre_animation": [
          "variable.num_professions = 15;",
          "variable.num_tiers = 3;",
          "variable.profession_index = (query.variant < variable.num_professions ? query.variant : 0);",
          "variable.level_index = query.trade_tier;"
        ],
        "animate": [
          {"cont_general": "q.property('hp4_paint:is_painting') == false"},
          {"cont_move": "q.property('hp4_paint:is_painting') == false"},
          {"cont_raise_arms": "q.property('hp4_paint:is_painting') == false"},
          {"cont_baby": "q.property('hp4_paint:is_painting') == false"},
          {"painting": "q.property('hp4_paint:is_painting') == true"}
        ]
      },
      "animations": {
        "cont_general": "controller.animation.villager_v2.general",
        "cont_move": "controller.animation.villager_v2.move",
        "cont_raise_arms": "controller.animation.villager_v2.raise_arms",
        "cont_baby": "controller.animation.villager_v2.baby",
        "general": "animation.villager.general",
        "look_at_target": "animation.common.look_at_target",
        "move": "animation.villager.move",
        "raise_arms": "animation.villager.raise_arms",
        "baby_transform": "animation.villager.baby_transform",
        "get_in_bed": "animation.villager.get_in_bed",
        "painting": "animation.hp4_paint.artist_villager.painting"
      },
      "render_controllers": [
        {"controller.render.default": "!q.property('hp4_paint:camouflage')"},
        {"controller.render.hp4_paint.artist_villager.camouflage": "q.property('hp4_paint:camouflage')"}
      ],
      "spawn_egg": {
        "texture": "spawn_egg_villager"
      }
    }
  }
